{
    "extends": ["airbnb", "airbnb/hooks", "plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "error"
    },
    "env": {
        "es6": true,
        "browser": true,
        "jest/globals": true
    }
}
