{
    "extends": ["airbnb", "airbnb/hooks"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "error",
        "jsx-a11y/label-has-associated-control": "off"
    },
    "env": {
        "es6": true,
        "browser": true
    }
}
